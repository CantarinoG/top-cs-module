{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "eqeqeq":"off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "class-methods-use-this": "warn"
    }
}
